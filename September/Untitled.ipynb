   "source": [
    "def maxProduct(nums):\n",
    "    max_prod = 1\n",
    "    prod = 1\n",
    "    for num in nums:\n",
    "        prod = prod*num\n",
    "        max_prod = max(prod, max_prod)\n",
    "        if prod < 1:\n",
    "            prod = 1\n",
    "    return max_prod\n",
    "\n",
    "maxProduct([2,3,-2,-2,4])"
   ]
  }
